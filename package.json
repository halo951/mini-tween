{
    "name": "small-tween",
    "version": "1.0.0",
    "description": "fork from @tween/tween.js(v25.0.0), and rebuild with tsup.",
    "keywords": [
        "axios",
        "plugins"
    ],
    "license": "MIT",
    "author": {
        "name": "halo951",
        "url": "https://github.com/halo951"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/halo951/axios-plugins.git"
    },
    "bugs": {
        "url": "https://github.com/halo951/axios-plugins/issues"
    },
    "sideEffects": false,
    "files": [
        "src",
        "dist"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "typings": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        },
        "./mini": {
            "types": "./dist/mini.d.ts",
            "require": "./dist/mini.cjs",
            "import": "./dist/mini.mjs"
        },
        "./easing/*": {
            "types": "./dist/easing/*.d.ts",
            "require": "./dist/easing/*.cjs",
            "import": "./dist/easing/*.mjs"
        },
        "./interpolation/*": {
            "types": "./dist/interpolation/*.d.ts",
            "require": "./dist/interpolation/*.cjs",
            "import": "./dist/interpolation/*.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "index": [
                "dist/typings/index.d.ts"
            ],
            "easing/*": [
                "src/easing/*.ts"
            ],
            "interpolation/*": [
                "src/interpolation/*.ts"
            ]
        }
    },
    "scripts": {
        "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
        "build": "unbuild",
        "format": "prettier -w **.ts **.md **.json"
    },
    "peerDependencies": {},
    "devDependencies": {
        "fast-glob": "^3.3.2",
        "typescript": "^5.6.2",
        "unbuild": "^2.0.0"
    },
    "dependencies": {}
}
